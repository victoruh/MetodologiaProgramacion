317         int contadorColumna=0;\par
318         int numCadenasGrises = 0;\par
319         //Cadena de caracteres para la lectura del fichero grises\par
320         char buffer [500];\par
321 \par
322         //Abrimos flujo entrada para leer del fichero\par
323         ifstream fi;\par
324         fi.open(fichero);\par
325 \par
326         if (fi) \{\par
327                 fi.getline(buffer,500); //lee la primera linea que es basura\par
328                 fi>>numCadenasGrises;\par
329                 fi.getline(buffer,500); //Leemos la basura seguida del 4 que es una linea entera\par
330         \}\par
331         else\{\par
332                 std::cerr << "Error de lectura del fichero" << std::endl;\par
333                 return false;\par
334         \}\par
